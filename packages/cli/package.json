{
  "name": "flags-cli",
  "version": "0.0.1",
  "description": "CLI tool for Flags SDK to generate types",
  "type": "module",
  "bin": {
    "flags-cli": "./dist/index.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "bun build ./src/index.ts --outfile=dist/index.js --target=node",
    "start": "bun run ./src/index.ts",
    "test": "bun test"
  },
  "dependencies": {
    "commander": "^12.0.0",
    "flags-core": "workspace:*"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/node": "^20.0.0",
    "typescript": "^5.0.0"
  }
}
